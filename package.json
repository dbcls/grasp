{
  "scripts": {
    "start": "npm run build && node --es-module-specifier-resolution=node main.js",
    "dev": "node --loader ts-node/esm --es-module-specifier-resolution=node main.ts",
    "watch": "nodemon -e ts,graphql",
    "test": "jest ${1} --detectOpenHandles",
    "test-watch": "jest ${1} --watch --detectOpenHandles",
    "build": "tsc",
    "docs": "typedoc"
  },
  "pre-commit": [
    "build",
    "test"
  ],
  "dependencies": {
    "@rdfjs/types": "^1.0.1",
    "@tsconfig/node14": "^1.0.0",
    "@types/lodash": "^4.14.178",
    "@types/lru-cache": "^7.4.0",
    "@types/node": "^16.11.10",
    "@types/sparql-http-client": "^2.2.7",
    "apollo-server": "^3.3.0",
    "apollo-server-express": "^3.3.0",
    "dataloader": "^2.0.0",
    "graphql": "^15.4.0",
    "graphql-tag": "^2.11.0",
    "handlebars": "^4.7.7",
    "lodash": "^4.17.21",
    "lru-cache": "^7.5.1",
    "nodemon": "^2.0.6",
    "outdent": "^0.8.0",
    "pino": "^7.6.2",
    "rdf-literal": "^1.3.0",
    "sparql-http-client": "^2.4.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@types/sparqljs": "^3.1.3",
    "jest": "^27.4.4",
    "jest-rdf": "^1.7.0",
    "rdf-quad": "^1.5.0",
    "sparqljs": "^3.5.1",
    "ts-jest": "^27.1.1",
    "typedoc": "^0.22.13"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "test/tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true,
    "setupFilesAfterEnv": [
      "jest-rdf"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/test/test-helpers.ts"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/test/test-helpers.ts"
    ],
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=14"
  },
  "type": "module"
}
